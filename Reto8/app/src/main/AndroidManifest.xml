<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reto8">

    <application
        android:allowBackup="false"
        android:icon="@drawable/fairy"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="false"
        android:theme="@style/Theme.Reto8">
        <activity
            android:name="com.example.reto8.FiltroClass"
            android:exported="false" />
        <activity
            android:name="com.example.reto8.Filtros"
            android:exported="false" />
        <activity
            android:name="com.example.reto8.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.reto8.EmpresaActivity"
            android:exported="false" />
    </application>

</manifest>